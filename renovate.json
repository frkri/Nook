{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
	"onboarding": false,
	"platform": "github",
	"includeForks": true,
	"packageRules": [
		{
      "matchUpdateTypes": ["minor", "patch"],
      "matchCurrentVersion": "!/^0/",
      "automerge": true
		}
	]
}
